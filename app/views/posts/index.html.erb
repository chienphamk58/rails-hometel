<h1>Posts#index</h1>
<p>Find me in app/views/posts/index.html.erb</p>
<h1>Listing Books</h1>

<table>
  <thead>
  <tr>
    <th>ID</th>
    <th>post_address</th>
    <th>post_intro</th>
    <th>post_price</th>
    <th>post_phone</th>
    <th>post_picture</th>
    <th>user_id</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @post.each do |post| %>
      <tr>
        <td><%=  %></td>
        <td><%= post.post_address %></td>
        <td><%= post.post_intro %></td>

        <td><%= post.post_price %></td>
        <td><%= post.post_phone %></td>
        <td><%= post.post_picture %></td>
        <td><%= post.user_id %></td>
        <td><%= link_to 'Show', post %></td>
        <td><%= link_to 'Edit', edit_post_path(post) %></td>
        <td><%= link_to 'Destroy', post, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
  <% end %>
  </tbody>
</table>
